<databaseChangeLog xmlns="http://www.liquibase.org/xml/ns/dbchangelog" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                   xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-latest.xsd">
    <changeSet id="create repository_secret table" author="danail.zlatanov">
        <preConditions onError="MARK_RAN" onFail="MARK_RAN">
            <not>
                <tableExists tableName="repository_secret"/>
            </not>
        </preConditions>
        <createTable tableName="repository_secret">
            <column name="repository_id" type="uuid"/>
            <column name="secret_id" type="uuid"/>
        </createTable>
    </changeSet>
</databaseChangeLog>